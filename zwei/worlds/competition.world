<?xml version="1.0"?>
<sdf version="1.7">
  <world name="competition_world">
    <!-- 物理引擎设置 -->
    <physics name="default_physics" default="true" type="ode">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1</real_time_factor>
    </physics>
    
    <!-- 环境光照 -->
    <include>
      <uri>model://sun</uri>
    </include> -
    
    <!-- 地面平面（可选）-->
    <include>
      <uri>model://ground_plane</uri>
      <name>ground</name>
    </include> 
  </world>
</sdf>
